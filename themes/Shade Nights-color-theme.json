{
	"name": "Shade Nights",
	"type": "dark",
	"colors": {
		"editor.background": "#261C2C",
		"editor.foreground": "#eeffff",
		"activityBarBadge.background": "#00ADB5",
		"sideBarTitle.foreground": "#bbbbbb",
		"activityBar.background": "#261C2C",
		"sideBar.background": "#1f1724",
		"button.background": "#bc7cc4",
		"button.hoverBackground": "#a36ca8",
		"titleBar.activeBackground": "#261C2C",
		"statusBar.background": "#26C485",
		"panel.background": "#1f1724",
		"tab.activeBackground": "#261C2C",
		"tab.inactiveBackground": "#2c2033",
		"terminal.foreground": "#eeffff",
		"activityBar.foreground": "#ff298d",
		"activityBar.inactiveForeground": "#ff298d7c",
		"editorWidget.background": "#1f1724",
		"sideBarSectionHeader.background": "#32253a",
		"editorGroupHeader.tabsBackground": "#1f1724",
		"editorGroupHeader.noTabsBackground": "#1f1724",
		"editorHoverWidget.background": "#34283b",
		// Notifications
		"notifications.background": "#493755",
		"notifications.foreground": "#eeffff",
		"input.background": "#43334d",
		"editor.selectionBackground": "#6448758f",
		"menu.background": "#302438",
		"menu.selectionBackground": "#4a3757",
		// Terminal
		"terminalCursor.foreground": "#c0bdbd",
		// Breadcrumbs
		"breadcrumb.background": "#32253a",
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#546E7A"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#F564A9"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#23bcc4"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name":"Separator And Boolean",
			"scope":[
				"punctuation.separator.comma",
				"punctuation.separator.period",
				"punctuation.separator.semicolon",
				"constant.language.boolean",
				"keyword.operator.assignment",
				"keyword.operator",
				"punctuation.terminator.statement"

			],
			"settings":{
				"foreground":"#00A676"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#9485eb"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#AFA5E7"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#1f84ab"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#1f84ab"
			}
		},
		{
			"name": "Number, Constant, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#6fd0dd"
			}
		},
		{
			"name": "Function Argument",
			"scope": [
				"variable.parameter"
			],
			"settings": {
				"foreground": "#e752a9"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#ffd3a1"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
